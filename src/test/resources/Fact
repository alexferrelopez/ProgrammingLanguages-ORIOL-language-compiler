miau: ranch() {
    miau num is 5;
    miau result is 1;

    // MULTIPLIQUEM perque volem el fact de 5 per EXEMPLE
    result is result times num;  // result = 1 * 5 = 5
    num is num minus 1;          // num = 5 - 1 = 4

    result is result times num;  // result = 5 * 4 = 20
    num is num minus 1;          // num = 4 - 1 = 3

    result is result times num;  // result = 20 * 3 = 60
    num is num minus 1;          // num = 3 - 1 = 2

    result is result times num;  // result = 60 * 2 = 120
    num is num minus 1;          // num = 2 - 1 = 1

    result is result times num;  // result = 120 * 1 = 120

    poop result;  // retorna el valor de 120
}
